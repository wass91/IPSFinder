<p id="notice"><%= notice %></p>

<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><h3>EPS</h3></div>

    <table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eps.each do |ep| %>
      <tr>
        <td><%= ep.nombre %></td>
        <td><%= link_to 'Ver', ep %></td>
        <td><%= link_to 'Editar', edit_ep_path(ep) %></td>
        <td><%= link_to 'Eliminar', ep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'Nueva EPS', new_ep_path %>
